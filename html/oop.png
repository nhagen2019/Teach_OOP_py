        let currentLanguage = currentPath[1].length === 2 ? `/${currentPath[1]}` : '';
        let newLocation;
    
        // Set new location
        if(specialMetros[currentLocation]) {
            console.log('edge case')
            // if location is one of the edge cases, use value from specialMetros object
            newLocation = specialMetros[currentLocation];
        } else {
            console.log('normal case')
            // if not edge case, reverse state-city to city-state
            let dashIndex = currentLocation.indexOf('-');
            let state = currentLocation.substr(0, dashIndex);
            let city = currentLocation.substr(dashIndex+1);
            newLocation = `${city}-${state}`;
        }
        // End of getting location and language (if language options exist for current country)
    
        // Start of setting job anchor link
        function setJobAnchorHrefs(jobPosition) {
